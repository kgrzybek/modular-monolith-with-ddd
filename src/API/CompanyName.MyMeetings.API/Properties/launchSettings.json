      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "api/values",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "DevWorkshops.Meetings.API": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
